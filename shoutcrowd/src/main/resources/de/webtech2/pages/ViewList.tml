<html t:type="layout" title="About shoutcrowd"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">

    <body>
        <div id="cwrapper">
            <h1>View: ${view}</h1>

            
            <t:if test="hasResults">
                Anzahl: ${resultCount}
                <p:else>
                    Keine Eintr√§ge!
                </p:else>
            </t:if>

            <t:Loop t:source="userList" t:value="userEntry"> 
                <t:ProfileListItem userId="userEntry.id" />
            </t:Loop>
        </div>
    </body>

</html>
