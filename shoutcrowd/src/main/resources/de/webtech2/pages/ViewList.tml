<html t:type="layout" title="About shoutcrowd"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">

    <body>
        <div id="cwrapper">
            <h1>ViewMode: ${viewMode}</h1>
            <h1>View: ${view}</h1>

            <t:Loop t:source="userList" t:value="userEntry"> 
                <t:ProfileListItem userId="userEntry.id" view="viewMode" />
            </t:Loop>
        </div>
    </body>

</html>
